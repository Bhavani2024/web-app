    "from sklearn import linear_model\n",
    "import pandas as pd\n",
    "\n",
    "#data recorded in csv format\n",
    "df = pd.read_excel(\"D:/pred.xlsx\")\n",
    "\n",
    "a = df[['year', 'adCost']]\n",
    "b = df['sale']\n",
    "\n",
    "R = linear_model.LinearRegression()\n",
    "R.fit(a, b)\n",
    "newSale = R.predict([[2024, 336]])\n",
    "#print the predicted sale\n",
    "print(newSale)"
